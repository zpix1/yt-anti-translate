{
  "name": "YouTube Anti Translate - mv3",
  "author": "zpix1; ported by namakeingo",
  "version": "1.17.0",
  "description": "A small extension to disable YT video titles autotranslation.",
  "manifest_version": 3,
  "browser_specific_settings": {
    "gecko": {
      "id": "youtube-anti-translate@namakeingo.co.uk",
      "strict_min_version": "109.0"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "src/start.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "src/background.js",
        "src/background_audio.js",
        "src/background_description.js"
      ],
      "matches": [
        "*://*.youtube.com/*"
      ]
    }
  ],
  "background": {
    "scripts": [
      "src/background.js",
      "src/background_audio.js",
      "src/background_description.js"
    ]
  },
  "action": {
    "default_popup": "pages/popup.html"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "host_permissions": [
    "*://*.youtube.com/*"
  ],
  "icons": {
    "48": "icon48.png"
  }
}