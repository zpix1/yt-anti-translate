{
  "name": "YouTube Anti Translate",
  "author": "zpix1",
  "version": "1.18.5",
  "description": "A small extension to disable YouTube video titles/audio/descriptions autotranslation.",
  "manifest_version": 3,
  "browser_specific_settings": {
    "gecko": {
      "id": "youtube-anti-translate-mv3@namakeingo.co.uk",
      "strict_min_version": "109.0"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "src/global.js",
        "src/content_injectglobal.js"
      ],
      "run_at": "document_end"
    },
    {
      "matches": [
        "*://*.youtube.com/*"
      ],
      "js": [
        "src/content_start.js",
        "src/content_channelbranding.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "src/global.js",
        "src/background.js",
        "src/background_audio.js",
        "src/background_description.js",
        "src/background_notifications.js"
      ],
      "matches": [
        "*://*.youtube.com/*"
      ]
    }
  ],
  "action": {
    "default_popup": "pages/popup.html"
  },
  "options_page": "pages/popup.html",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://*.youtube.com/*"
  ],
  "icons": {
    "48": "icon48.png"
  }
}